    "checkver": {
        "url": "https://unity.com/releases/editor/lts-releases.xml",
        "regex": "\"unityhub:\/\/2020(.[0-9]+.*?)\/(.*)&lt;\"",
        "replace": "2020${1}"
    },
    "autoupdate": {
        "url": "https://download.unity3d.com/download_unity/$match2/Windows64EditorInstaller/UnitySetup64-2020$match1.exe"
    }
